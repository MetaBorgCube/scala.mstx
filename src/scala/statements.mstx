import scala.def
import scala.iimport

stmts-ok(s_obj, stmts) :- stmts match
  { [] -> true
  | stmt:stmts ->
      stmt-ok(s_obj, stmt)
    , stmts-ok(s_obj, stmts)
  }.

stmt-ok(s_obj, stmt) :- stmt match
  { DF2TM(annot, modif, def) ->
      def-ok(s_obj, def)
  | IM2TM(ImportExpr(stable_id, grp)) ->
      iimports-stable-id(s_obj, stable_id, grp)
  | _ -> false // FIXME
  }.
